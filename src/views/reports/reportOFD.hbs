
<section class="report">
    <nav>
        <div class="nav-wrapper">
            <a href="/" class="brand-logo ml-3">Отчет по ОФД</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="/">Главная</a></li>
                <li><a href="/auth/logout">Выйти</a></li>
            </ul>
        </div>
    </nav>
    <div class="row">
        <div class="col s10 offset-s1">
            <table class="table highlight centered" id="tableOFD" cellspacing="0" width="100%" data-page-length='15'>
                <thead>
                    <tr>
                        <th>Название</th>
                        <th>Касса</th>
                        <th>Адрес ОФД</th>
                        <th>Логин</th>
                        <th>Пароль</th>
                        <th>Срок</th>
                        <th>Дата <br> активации</th>
                        <th>Дата <br> окончания</th>
                        <th>Истекает</th>
                    </tr>
                </thead>
                <tbody class="main-table" id="table-contacts">
                    {{#each dataOFD}}
                        {{#each ofd}}
                            <tr>
                                <td>{{ ../name }}</td>
                                <td>{{ ofdkass }}</td>
                                <td>{{addressOfd}}</td>
                                <td>{{ofdLogin}}</td>
                                <td>{{ofdPassword}}</td>
                                <td>{{ofdTerm}}</td>
                                <td>{{ofdTime}}</td>
                                <td></td>
                                <td></td>
                            </tr>                                          
                        {{/each}}
                    {{/each}}
                </tbody>
            </table>
        </div>
        </div>
    </div>
</section>