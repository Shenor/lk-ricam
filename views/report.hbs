
<article>
    <nav>
        <div class="nav-wrapper">
            <a href="#" class="brand-logo">Отчет по фискальным данным</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="/">Главная</a></li>
                <li><a href="/auth/logout">Выйти</a></li>
            </ul>
        </div>
    </nav>
    <div class="row">
        <div id="test1" class="col s12">
            <table class="table highlight centered" id="example" cellspacing="0" width="100%" data-page-length='15'>
                <thead>
                                <tr>
                                    <th>Название</th>
                                    <th>Касса</th>
                                    <th>Модель</th>
                                    <th>Серийный <br> номер</th>
                                    <th>Срок(мес.)</th>
                                    <th>S/N</th>
                                    <th>Дата <br> установки</th>
                                    <th>Дата <br> окончания</th>
                                    <th>Истекает</th>
                                </tr>
                </thead>
                <tbody class="main-table" id="table-contacts">
                    {{#each resault}}

                        {{#each fiscal}}
                            <tr>
                                <td>{{ ../name }}</td>
                                <td>{{ fiscalRegistr.fiscalOfd }}</td>
                                <td>{{ fiscalRegistr.fiscalModel }}</td>
                                <td>{{ fiscalRegistr.fiscalSN }}</td>
                                <td>{{ fiscalDrive.fiscalTime }}</td>
                                <td>{{ fiscalDrive.fiscalSN  }}</td>
                                <td data-order={{ date.fiscalDateUnix }}>{{ fiscalDrive.fiscalDate }}</td>
                                <td data-order={{ date.fEndDateUnix }}>{{ date.fEndDate }}</td>
                                <td>{{ date.fLeftDate }}</td>
                            </tr>                                    
                        {{/each}}              
                    {{/each}}
                </tbody>
            </table>
        </div>
        </div>
    </div>
</article>