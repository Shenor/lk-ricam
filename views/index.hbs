        <div class="modal" id="modal-delete">
          <a href="#close" class="modal-overlay" aria-label="Close"></a>
          <div class="modal-container">
            <div class="modal-header">
              <a href="#close" class="btn btn-clear float-right" aria-label="Close"></a>
              <div class="modal-title h5">Удалить клиента</div>
            </div>
            <div class="modal-body">
              <div class="content">
                <!-- content here -->
                <div class="form-group text-dark text-left">
                  <label class="form-label h6" for="delete-name">Введите название клиента, которого вы хотите
                    удалить.</label>
                  <input class="form-input" type="text" id="delete-name" placeholder="Введите название">
                </div>
              </div>
            </div>
            <div class="modal-footer">
              <button class="btn btn-primary button-delete">Удалить</button>
              <button class="btn btn-primary btn-close">Закрыть</button>
            </div>
          </div>
        </div>
        <!-- End Modal Delete-->
        
